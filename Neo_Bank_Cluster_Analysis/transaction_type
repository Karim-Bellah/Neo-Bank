SELECT user_id,
  IFNULL(FEE, 0) AS FEE,
  IFNULL(TOPUP, 0) AS TOPUP,
  IFNULL(EXCHANGE, 0) AS EXCHANGE,
  IFNULL(TRANSFER, 0) AS TRANSFER,
  IFNULL(CARD_PAYMENT, 0) AS CARD_PAYMENT,
  IFNULL(CARD_REFUND, 0) AS CARD_REFUND,
  IFNULL(ATM, 0) AS ATM,
  IFNULL(REFUND, 0) AS REFUND,
  IFNULL(CASHBACK, 0) AS CASHBACK,
  IFNULL(TAX, 0) AS TAX

FROM my-project-study-393315.neo_bank.cluster_test_transaction_type
PIVOT (SUM(user_id_count) FOR transactions_type IN ("FEE", "TOPUP", "EXCHANGE", "TRANSFER", "CARD_PAYMENT", "CARD_REFUND", "ATM", "REFUND", "CASHBACK", "TAX"))